<% @travels.each do |t| %>
  <p><%= t.title %></p>
  <% if t.travel_image.url.nil? %>
    <p><%= link_to image_tag('blank.png'), t %></p>
  <% else %>
    <p><%= link_to image_tag(t.travel_image.url), t %></p>
  <% end %>
  <p><%= t.departured_at %></p>
  <p><%= t.returend_at %></p>
<% end %>

<p><%= link_to "NEW", new_travel_path %></p>
